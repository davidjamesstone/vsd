{
  "name": "vsd",
  "version": "0.0.13",
  "description": "Visual hapi + mongoose IDE",
  "main": "index.js",
  "bin": {
    "vsd": "bin/vsd.js"
  },
  "scripts": {
    "build:js": "bin/build-js",
    "clean:admin": "bin/clean-admin",
    "copy:admin": "bin/copy-admin",
    "build:css": "bin/build-css",
    "build": "bin/build",
    "watch": "bin/watch",
    "lint": "standard",
    "create:example": "cd docs && ../bin/vsd.js -c demo",
    "clone:todo": "cd docs && git clone https://github.com/davidjamesstone/vsd-example-todo",
    "test": "npm run lint",
    "xprepublish": "npm run build",
    "xpostinstall": "npm run create:example && npm run clone:todo",
    "start": "node index"
  },
  "license": "ISC",
  "dependencies": {
    "admin-lte": "2.4.2",
    "boom": "7.1.1",
    "bossy": "4.0.1",
    "dagre": "0.8.1",
    "filesize": "3.5.11",
    "glupe": "1.0.3",
    "good": "8.0.0-rc1",
    "good-console": "7.0.0",
    "good-file": "6.0.1",
    "good-squeeze": "5.0.2",
    "handlebars": "4.0.11",
    "inert": "5.1.0",
    "joi": "13.1.0",
    "nes": "7.0.2",
    "require-directory": "2.1.1",
    "screenfull": "3.3.2",
    "standard": "10.0.3",
    "standard-format": "2.2.4",
    "vision": "5.3.1",
    "vsd-plugin-fs": "file:../vsd-plugin-fs",
    "vsd-plugin-fs-watch": "file:../vsd-plugin-fs-watch"
  },
  "devDependencies": {
    "browserify": "15.2.0",
    "dagre-d3": "0.6.0",
    "document-register-element": "1.7.0",
    "exorcist": "1.0.0",
    "incremental-dom": "0.5.1",
    "node-sass": "4.7.2",
    "page": "1.8.0",
    "supermodels.js": "0.2.1",
    "superviewify": "3.0.0",
    "wisk": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/davidjamesstone/vsd.git"
  },
  "bugs": {
    "url": "http://github.com/davidjamesstone/vsd/issues",
    "email": "davidjamesstone@gmail.com"
  },
  "author": "David Stone <davidjamesstone@gmail.com>",
  "standard": {
    "ignore": [
      "server/public"
    ]
  }
}
